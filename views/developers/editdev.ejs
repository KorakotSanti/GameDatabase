<%- include('../partials/header') %>
<div class="grid d-flex justify-content-center border-top border-primary">
    <form action="/developer/<%= info['dev_id'] %>?_method=PUT" method="POST" class="form-css">
       <div class="form-group">
        <h3>Edit: <%= info['dev_name'] %></h3>
       </div>
       <div class="form-group">
        <label>Developer:</label>
        <input type="text" name="devname" class="form-control" required value="<%= info['dev_name'] %>" required>
       </div>
       <div class="form-group">
        <label>Year Established</label>
        <input type="text" aria-describedby="yr" name="est" class="form-control" pattern="\d.{0,4}" title="0 or 4 digit" value=<%= info['est'] %>>
        <small id="yr" class="form-text text-muted">Leave Blank or Enter 4 digit number</small>
       </div>
       <div class="row d-flex justify-content-center">
          <button class="btn btn-success">Submit</button>
       </div>
    </form>
</div>
<%- include("../partials/footer") %>